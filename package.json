    "react": "^16.12.0",
    "react-dom": "^16.12.0",
    "react-ga4": "^1.4.1",
    "react-google-recaptcha": "^2.1.0",
    "react-redux": "^7.2.1",
    "redux": "^4.0.5",
    "ts-node": "^10.2.1"
  },
  "devDependencies": {
    "@babel/core": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/preset-env": "^7.8.3",
